{{#>dialogueBubble}}
	{{#*inline 'title'}}
		<div class="qm-wrapper">
			{{translate 'dialogue.licence-headline'}}
			{{>questionMark target='qm-licence'}}
		</div>

		<div id="qm-licence" class="question-mark-content">
			{{translate 'dialogue.licence-question-mark'}}
		</div>
	{{/inline}}

	{{#*inline 'content'}}
		<div class="form-group">
			<label><input type="checkbox" name="licence" value="{{original.getId}}"> {{original.getName}}</label>
		</div>

		{{#each compatibles}}
			<div class="form-group">
				<label><input type="checkbox" name="licence" value="{{this.getId}}"> {{this.getName}}</label>
			</div>
		{{/each}}
	{{/inline}}
{{/dialogueBubble}}
